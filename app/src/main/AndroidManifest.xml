<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.brynmawr.cmsc353.webapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleWebApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".SelectDonationSuccess"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DonationFeedActivity" />
        <activity android:name=".UnimplementedActivity" />
        <activity android:name=".CreateDonationActivity" />
        <activity android:name=".ViewDonationActivity" />
        <activity android:name=".NewDonationMadeActivity" />
    </application>

</manifest>